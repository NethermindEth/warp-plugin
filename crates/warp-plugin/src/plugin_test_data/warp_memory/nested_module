//! > Test expansion of a function signature depending on warp memory.

//! > test_runner_name
ExpandContractTestRunner

//! > cairo_code
#[contract]
mod Module1{
	mod Module{
        use warplib::memory::WarpMemoryTrait;

		#[implicit(warp_memory, Felt252Dict<u128>)]
		fn read_from_memory(index: felt252) {
			warp_memory.read_u128(index);
		}
	}
}

//! > expected_diagnostics

//! > generated_cairo_code
#[contract]
mod Module1 {
    mod Module {
        use warplib::memory::WarpMemoryTrait;

        fn read_from_memory(ref warp_memory: Felt252Dict<u128>, index: felt252) {
            warp_memory.read_u128(index);
        }
    }
}
